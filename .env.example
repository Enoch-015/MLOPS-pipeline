# ClearML Configuration
CLEARML_HOST_IP=localhost
CLEARML_WEB_HOST=http://localhost:8080
CLEARML_FILES_HOST=http://localhost:8081

# Optional: Set these if you want to use specific access keys
# CLEARML_API_ACCESS_KEY=your_access_key_here
# CLEARML_API_SECRET_KEY=your_secret_key_here
# CLEARML_AGENT_ACCESS_KEY=your_agent_access_key_here
# CLEARML_AGENT_SECRET_KEY=your_agent_secret_key_here

# Git credentials for agent (optional)
# CLEARML_AGENT_GIT_USER=your_git_username
# CLEARML_AGENT_GIT_PASS=your_git_password

# AWS credentials (optional)
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_DEFAULT_REGION=us-east-1

# Database Configuration
DB_HOST=your_database_host
DB_PORT=5432
DB_NAME=your_database_name
DB_USERNAME=your_username
DB_PASSWORD=your_password

# ETL Configuration
ETL_CHUNK_SIZE=10000
ETL_TABLE_NAME=beverage_sales
ETL_BATCH_SIZE=1000
ETL_MAX_RETRIES=3
ETL_RETRY_DELAY=5

# Logging Configuration
LOG_LEVEL=INFO

# Optional: Environment identifier
ENVIRONMENT=development
